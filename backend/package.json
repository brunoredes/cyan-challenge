  "scripts": {
    "start": "cross-env NODE_ENV=production sucrase-node ./src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon ./src/server.js",
    "pretest": "cross-env NODE_ENV=test yarn run sequelize db:migrate",
    "test": "cross-env NODE_ENV=test jest --verbose",
    "posttest": "cross-env NODE_ENV=test yarn run sequelize db:migrate:undo:all",
    "init": "docker-compose up -d && yarn install && yarn dev",
    "manual": "docker run --name cyan-mills -e POSTGRES_PASSWORD=docker POSTGRES_DB=cyan-challenge -p 5432:5432 -d postgis/postgis",
    "migrate": "cross-env NODE_ENV=development yarn sequelize db:migrate"
  },
